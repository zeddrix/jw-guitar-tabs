{
	"name": "jw-guitar-tabs_svelte-web",
	"version": "1.5.0",
	"description": "JW Guitar Tabs is a website where people who love our JW songs can enjoy playing the simplest tabs on their guitar.",
	"license": "MIT",
	"repository": "https://github.com/zeddrix/jw-guitar-tabs_svelte-web",
	"scripts": {
		"dev": "svelte-kit dev -p 3030",
		"build": "npm run smui-light && rm -rf build && svelte-kit build --verbose && touch build/.nojekyll",
		"deploy": "npm run build && npx gh-pages -d build -t true",
		"preview": "svelte-kit preview -p 3030",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
		"prepare": "npm run smui-light && npm run smui-dark",
		"smui-light": "smui-theme compile static/smui.css -i src/theme",
		"smui-dark": "smui-theme compile static/smui-dark.css -i src/theme/dark"
	},
	"devDependencies": {
		"@smui/banner": "^6.0.0-beta.13",
		"@smui/button": "^6.0.0-beta.13",
		"@smui/common": "^6.0.0-beta.13",
		"@smui/drawer": "^6.0.0-beta.13",
		"@smui/icon-button": "^6.0.0-beta.13",
		"@smui/list": "^6.0.0-beta.13",
		"@smui/menu": "^6.0.0-beta.13",
		"@smui/textfield": "^6.0.0-beta.13",
		"@smui/top-app-bar": "^6.0.0-beta.13",
		"@sveltejs/adapter-static": "^1.0.0-next.26",
		"@sveltejs/kit": "next",
		"@typescript-eslint/eslint-plugin": "^5.10.1",
		"@typescript-eslint/parser": "^5.10.1",
		"eslint": "^8.8.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^3.4.0",
		"fomantic-ui": "^2.8.8",
		"prettier": "^2.5.1",
		"prettier-plugin-svelte": "^2.6.0",
		"sass": "^1.49.0",
		"smui-theme": "^6.0.0-beta.13",
		"svelte": "^3.46.3",
		"svelte-carousel": "^1.0.16",
		"svelte-device-detector": "^0.2.2",
		"svelte-preprocess": "^4.10.2",
		"svelte-typeahead": "^4.2.1",
		"tslib": "^2.3.1",
		"typescript": "^4.5.5"
	},
	"type": "module",
	"dependencies": {
		"colors": "^1.4.0",
		"dotenv": "^15.0.0",
		"gh-pages": "^3.2.3",
		"lodash": "^4.17.21",
		"mongodb": "^4.3.1",
		"svelte-check": "^2.4.0"
	}
}
